syntax on

"colorscheme sublimemonokai
colorscheme sublimemonokai

set tabstop=4 
set shiftwidth=4 
set softtabstop=4 

set nocompatible
set number

set cursorline 
set hlsearch

set nobackup

nnoremap ]b :<C-u>bp<cr>
nnoremap [b :<C-u>bn<cr>

nnoremap [e :<C-u>cp<cr>
nnoremap ]e :<C-u>cn<cr>
nnoremap ]E :<C-u>copen<cr>

nnoremap <C-l> :<C-u>nohl<cr><C-l>
